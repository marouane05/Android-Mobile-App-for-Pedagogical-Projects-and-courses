            android:id="@+id/linearLayout4">

        <RelativeLayout android:layout_width="match_parent" android:layout_height="74dp">
            <TextView android:text="Module1 " android:textSize="18sp" android:layout_width="wrap_content"
                      android:layout_height="41dp"
                      android:layout_marginTop="10dp"
                      android:textColor="#1c60dd"/>
            <EditText android:id="@+id/module1"
                      android:hint="nom du module" android:layout_width="162dp" android:layout_height="wrap_content"
                      android:layout_marginLeft="80dp"/>
            <EditText
                    android:background="@drawable/blue_border_rounded_cornwe"
                    android:layout_width="89dp"
                    android:layout_height="62dp"
