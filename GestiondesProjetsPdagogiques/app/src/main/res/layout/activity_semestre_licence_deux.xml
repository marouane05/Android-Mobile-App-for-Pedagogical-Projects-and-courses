                    android:background="@drawable/blue_border_rounded_cornwe"
                    android:layout_width="89dp"
                    android:layout_height="62dp"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/heure5" android:layout_marginLeft="240dp"
